{
  "version": 2,
  "routes": [
    {
      "src": "/check-auth",
      "methods": ["GET"], // Adjust this to match your specific route method
      "dest": "server.js", // Change to your backend entry file
      "headers": {
        "Access-Control-Allow-Origin": "https://recordsfrontend.vercel.app",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE",
        "Access-Control-Allow-Headers": "Content-Type, Authorization"
      }
    }
  ]
}
